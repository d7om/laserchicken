<% provide(:title, @entry.title) %>
<% up = guess_a_parent_for(@entry) %>
<% provide(:subtitle, link_to_if(up.instance_of?(Subscription), up.title, up)) %>
<%= provide(:previous, link_to_previous) %>
<%= provide(:next, link_to_next) %>
<% provide(:filter_actions, entry_filter_actions) %>

<%= render @entry, show_complete: true %>
