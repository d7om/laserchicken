<% provide(:title, @entry.title) %>
<% provide(:subtitle, link_to(@entry.feed.url)) %>
<%= provide(:previous, link_to("<<", [:previous, @parent, @entry], rel: 'prev', class: 'action')) %>
<%= provide(:next, link_to(">>", [:next, @parent, @entry], rel: 'next', class: 'action')) %>

<%= render @entry, show_complete: true %>
